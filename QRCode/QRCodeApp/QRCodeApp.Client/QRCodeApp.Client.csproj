<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
    <StaticWebAssetProjectMode>Default</StaticWebAssetProjectMode>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="9.*" />
    <PackageReference Include="MudBlazor" Version="8.*" />
    <PackageReference Include="Syncfusion.Blazor.BarcodeGenerator" Version="28.1.39" />
    <PackageReference Include="Syncfusion.Blazor.Buttons" Version="28.1.39" />
    <PackageReference Include="Syncfusion.Blazor.DropDowns" Version="28.1.39" />
    <PackageReference Include="Syncfusion.Blazor.Inputs" Version="28.1.39" />
    <PackageReference Include="Syncfusion.Blazor.Themes" Version="28.1.39" />
  </ItemGroup>

</Project>
